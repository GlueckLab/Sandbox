<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='testclient'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->


 <!-- UC Denver Web Service Common api -->
 <inherits name="edu.ucdenver.bios.webservice.common.common"/>
 <!--  JSON  -->
 <inherits name="com.google.gwt.json.JSON" />
 <!--  RestyGWT
 <inherits name="org.fusesource/restygwt/RestyGWT" /> -->
 
 <inherits name="name.pehl.piriti.json.JSON" />
 
  <!-- Specify the app entry point class.                         -->
  <entry-point class='edu.ucdenver.testclient.client.TestClient'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
